<%= form_with(model: Tsubuyaki.new, local: true) do |form| %>
  <div class="tsubuyaki_title">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div class="tsubuyaki_content">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <%= form.submit %>
<% end %>
